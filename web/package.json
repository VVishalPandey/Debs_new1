  "config": {
    "sdk_generator_version": 2,
    "min_lighthouse_score": 0,
    "controlsAllPaths": true
  },
  "scripts": {
    "add:component": "sdk-generate component",
    "add:form": "sdk-generate form",
    "add:page": "sdk-generate page",
    "analyze-build": "MOBIFY_ANALYZE=true npm run prod:build",
    "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --dest=./app/static/svg/sprite-dist ./app/static/svg/sprite-source/*.svg",
    "clean:svgs": "svgo --enable='removeUselessStrokeAndFill' -f ./app/static/svg/sprite-source",
    "clean:images": "imagemin-power -v --recursive --plugin=pngquant \"./app/static/img/**/*\" --out-dir=.",
    "messaging:certificate": "sdk-messaging certificate",
    "messaging:testmessage": "sdk-messaging testmessage",
    "messaging:upload": "sdk-messaging upload",
    "copyright:lint": "copyright 'app/!(vendor|static)/**/*.js?(x)' 'worker/**/*.js?(x)' 'tests/**/*.js?(x)' 'webpack/**/*.js?(x)' 'service-worker-loader.js'",
    "copyright:fix": "copyright --fix --update 'app/!(vendor|static)/**/*.js?(x)' 'worker/**/*.js?(x)' 'tests/**/*.js?(x)' 'webpack/**/*.js?(x)' 'service-worker-loader.js'",
    "dev": "sdk-get-routes && sdk-create-hash-manifest && node ./dev-server/index.js",
    "im:docs": "jsdoc -c .jsdocrc",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:fix": "npm run lint:js -- --fix && sass-lint-auto-fix --config-sass-lint .sass-lint.yml \"app/**/*.scss\"",
    "lint:js": "eslint  \"**/*.{js,jsx}\"",
    "lint:sass": "sass-lint -c .sass-lint.yml \"app/**/*.scss\" -v",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "precommit": "lint-staged",
    "prepush": "npm run test:all",
    "styleguide": "styleguidist --config webpack/styleguide.config.js server",
    "save-credentials": "sdk-save-credentials",
    "smoke-test": "bash ./tests/system/smoke-test.sh",
    "test": "cross-env NODE_ENV=test jest",
    "test-ci": "cross-env NODE_ENV=test jest --maxWorkers=2",
    "test:all": "npm run lint && npm test -- --coverage",
    "test:server": "bash ./scripts/test-server.sh",
    "test:watch": "npm test -- --watch",
    "pa11y-sync-tasks-uk": "node ./tests/pa11y/dashboard-task-runner.js --syncTasks --websiteLocal UK",
    "pa11y-sync-tasks-roi": "node ./tests/pa11y/dashboard-task-runner.js --syncTasks --websiteLocal ROI",
    "pa11y-sync-tasks-int": "node ./tests/pa11y/dashboard-task-runner.js --syncTasks --websiteLocal INT",
    "test:accessibility-uk": "node ./tests/pa11y/runner.js --websiteLocal UK",
    "test:accessibility-roi": "node ./tests/pa11y/runner.js --websiteLocal ROI",
    "test:accessibility-int": "node ./tests/pa11y/runner.js --websiteLocal INT",
    "prod:build": "bash ./scripts/build.sh",
    "update-loader-routes": "sdk-get-routes",
    "generate:e2e-report": "node ./tests/system/nightwatch-reporter.js",
    "test:performance-local": "./node_modules/sitespeed.io/bin/sitespeed.js  --config ./tests/sitespeedio/local-config.json ./tests/sitespeedio/urls.txt",
    "test-debug": "./node_modules/.bin/node-debug ./node_modules/.bin/jest -i --watch",
    "ssr": "bash ./scripts/ssr.sh",
    "ssr:inspect": "bash ./scripts/ssr.sh inspect",
    "test:check-lighthouse-score": "node tests/performance/lighthouse/check-lighthouse-score.js",
    "test:sync-lighthouse-score": "node tests/performance/lighthouse/sync-lighthouse-score.js",
    "test:pwa-prod": "bash ./tests/performance/lighthouse/lighthouse-prod.sh",
    "test:pwa-preview": "bash ./tests/performance/lighthouse/lighthouse-preview.sh",
    "test:lighthouse": "node tests/performance/lighthouse/run-lighthouse.js",
    "test:automation-headless": "cross-env NODE_ENV=test TEST_PROFILE=${TEST_PROFILE=local} TEST_DATA_SET=${TEST_DATA_SET=DESKTOP} GRID=${GRID=FALSE} node ./node_modules/nightwatch/bin/runner.js -c ./tests/system/nightwatch-config.js --env remote --tag ${TAG=automationInProgress} --suiteRetries 1",
    "test:automation-coverage": "cross-env NODE_ENV=test TEST_DATA_SET=${TEST_DATA_SET=UK} node ./tests/system/feature-coverage.js",
    "test:automation": "cross-env NODE_ENV=test TEST_PROFILE=${TEST_PROFILE=local} TEST_DATA_SET=${TEST_DATA_SET=DESKTOP} GRID=${GRID=FALSE} PARALLEL_RUN=${PARALLEL_RUN=false} node ./node_modules/nightwatch/bin/runner.js -c ./tests/system/nightwatch-config.js -e ${INSTANCE=chrome} --tag ${TAG=automationInProgress} --suiteRetries 1",
    "test:automation-msedge": "cross-env NODE_ENV=test TEST_PROFILE=production TEST_DATA_SET=DESKTOP GRID=${GRID=FALSE} node ./node_modules/nightwatch/bin/runner.js -c ./tests/system/nightwatch-config.js -e msedge --tag e2e --suiteRetries 1",
    "storybook": "start-storybook -s ./app -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "globals": {
      "WEBPACK_SITE_URL": "https://www.debenhams.com/",
      "WEBPACK_PAGE_NOT_FOUND_URL": "/404.html",
      "PACKAGE_JSON_MOBIFY": "{}",
      "WEBPACK_PACKAGE_JSON_MOBIFY": "{}",
      "DEBUG": false
    },
    "cacheDirectory": "./node_modules/.cache",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!app/{loader.js,main.jsx}",
      "!app/static/js/*.js",
      "!app/vendor/*.js",
      "!app/containers/{templates,reducers}.js",
      "!app/**/{container,constants,selectors}.{js,jsx}",
      "!{dev-server,lighthouse,coverage,build,tests,webpack,worker}/**/*.js",
      "!styleguide/**/*.{js,jsx}",
      "!app/loader-routes.js",
      "!service-worker-loader.js",
      "!reports/*.{js,jsx}",
      "!app/integration-manager/docs/**/*.{js,jsx}",
      "!app/native/**/*.{js,jsx}",
      "!app/utils/magento-utils.js",
      "!app/config/translations.js",
      "!app/utils/ie-redirect.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 8,
        "functions": 11,
        "lines": 29,
        "statements": 20
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/tests/emptyStringMock.js",
      "^.+\\.scss$": "<rootDir>/tests/emptyStringMock.js",
      "^components(.*)$": "<rootDir>/app/components$1",
      "^connector-extension(.*)$": "<rootDir>/app/connector-extension$1",
      "^containers(.*)$": "<rootDir>/app/containers$1",
      "^stub-connector(.*)$": "<rootDir>/app/integration-manager/_stub-connector$1",
      "^utils(.*)$": "<rootDir>/app/utils$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jsdom/lib/jsdom/browser/resources/resource-loader))"
    ],
    "setupFiles": [
      "<rootDir>/tests/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/webpack/",
      "/tmp/",
      "/node_modules/",
      "/app/static/",
      "/app/vendor/",
      "/dev-server/",
      "/coverage/"
    ],
    "testRegex": "\\.?(test|spec)\\.jsx?$",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "./reports/"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,scss}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
