{
  "presets": [
    ["env", {"modules": false}],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-async-to-generator",
    "transform-class-properties",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node",
        "transform-class-properties"
      ],
      "presets": [
        "es2015",
        "react"
      ]
    }
  }
}
